
{
  "version": 2,
  "builds": [
    {
      "src": "commerce/wsgi.py",
      "use": "@vercel/python",
      "config": { 
        "maxLambdaSize": "15mb", "runtime": "python3.12", "buildCommand": "bash build.sh" }
    }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "commerce/wsgi.py"
    },
    {
        "src":"/static/(.*)",
        "dest":"/static/$1"
    }
  ]
}